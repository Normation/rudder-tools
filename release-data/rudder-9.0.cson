                  "debian-10":      [ "i386", "amd64", "armhf", "arm64" ],
                  "debian-11":      [ "i386", "amd64", "armhf", "arm64" ],
                  "debian-12":      [ "i386", "amd64", "armhf", "arm64" ],
                  "debian-13":      [ "amd64", "armhf", "arm64" ],
                  "ubuntu-18.04":   [ "amd64" ],
                  "ubuntu-20.04":   [ "amd64" ],
                  "ubuntu-22.04":   [ "amd64" ],
                  "ubuntu-24.04":   [ "amd64" ],
                  "rhel-7":         [ "amd64" ],
